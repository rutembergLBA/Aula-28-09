import java.util.Scanner;

public class For2{
    public static void main() {
        Scanner scanner = new Scanner (System.in);
        int x;
        int y;
    
        System.out.println("Imforme o primeiro número");
        x = scanner.nextInt();
        System.out.println("Imforme o segundo número número");
        y = scanner.nextInt();
        if (x<y){
            for (int i = x; i<=y; i++)
                System.out.println(i);
        }else{
            for (int i = x; i>=y; i--)
                System.out.println(i);
        }
    }
}
